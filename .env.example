# WZRD Strategy System Environment Variables

# Data Sources
POLYGON_API_KEY=your_polygon_api_key_here
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# Database (if using external database)
DATABASE_URL=postgresql://user:password@localhost:5432/wzrd_strategies

# Execution Settings
DEFAULT_INITIAL_CAPITAL=100000
DEFAULT_COMMISSION_PER_SHARE=0.0035
DEFAULT_SLIPPAGE_BPS=1

# Logging
LOG_LEVEL=INFO
LOG_FILE=wzrd_strategy.log

# Development
DEBUG=false
MOCK_DATA=true